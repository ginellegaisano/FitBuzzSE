<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.app" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.android.app.sleepChooserListActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.android.app.sleepChooserDetailActivity"
            android:label="@string/title_sleepchooser_detail"
            android:parentActivityName="com.example.android.app.sleepChooserListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.android.app.sleepChooserListActivity" />
        </activity>
        <activity
            android:name="com.example.android.app.ubermanScheduler"
            android:label="@string/title_activity_uberman_scheduler" >
        </activity>
        <activity
            android:name="com.example.android.app.schedulerTutorial"
            android:label="@string/title_activity_scheduler_tutorial" >
        </activity>
    </application>

</manifest>
